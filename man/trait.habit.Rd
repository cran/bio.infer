\name{trait.habit}
\alias{trait.habit}
\docType{data}
\title{Habit traits for benthic invertebrates}
\description{
  Habit classifications for different benthic invertebrates. 
}
\usage{data(trait.habit)}
\format{
  A data frame with two columns.
  \describe{
    \item{\code{TAXON}}{Character vector with the names of each taxon.}
    \item{\code{TRAITVAL}}{Value of trait classification}
  }
}
\details{
  These trait classifications were summarized from the USGS data set
  prepared by Viera et al. \url{http://pubs.usgs.gov/ds/ds187/}.
}
\source{
  Yuan, L.L.
}
\examples{
data(trait.habit)
}
\keyword{datasets}
